{
  "name": "wing-tron",
  "version": "1.0.0",
  "description": "a drawing combat game based on the famous 'tron'",
  "main": "lib/index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [],
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0-beta.53",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "babel-loader": "^8.0.0-beta.4",
    "concurrently": "^3.6.0",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "jest-canvas-mock": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "author": "js-jslog",
  "homepage": "https://github.com/js-jslog/wingTron#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-jslog/wingTron.git"
  },
  "dependencies": {
    "flux": "^3.1.3"
  }
}
